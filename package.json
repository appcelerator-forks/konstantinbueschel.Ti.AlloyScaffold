{
  "name": "tialloyscaffold",
  "version": "1.0.0",
  "description": "Titanium Mobile Alloy App",
  "keywords": [
    "titanium",
    "appcelerator",
    "alloy",
    "scaffold",
    "app"
  ],
  "author": {
    "name": "Konstantin Bueschel",
    "email": "konstantin.bueschel@gmail.com"
  },
  "homepage": "https://github.com/konstantinbueschel/Ti.AlloyScaffold",
  "repository": {
    "type": "git",
    "url": "https://github.com/konstantinbueschel/Ti.AlloyScaffold"
  },
  "bugs": {
    "url": "https://github.com/konstantinbueschel/Ti.AlloyScaffold/issues",
    "email": "konstantin.bueschel@gmail.com"
  },
  "main": "/app/alloy.js",
  "config": {
    "bowerDir": "./bower_components"
  },
  "scripts": {
    "debug:show": "find ./Resources ! -name \"q.js\" ! -name \"lodash.js\" ! -name \"async.js\" -name \"*.js\" -exec ti-stealth restore {} \\;",
    "debug:hide": "find ./Resources ! -name \"q.js\" ! -name \"lodash.js\" ! -name \"async.js\" -name \"*.js\" -exec ti-stealth enable {} -n error,info,warn \\;",
    "version:bump": "tich select",
    "postversion:bump": "gulp versioning",
    "clean:app": "appc ti clean",
    "clean:bower": "rm -rv $npm_package_config_bowerDir",
    "clean:dist": "find ./dist -name \"*.apk\" -o -name \"*.ipa\" -o -name \"*.app\" | xargs rm -Rfv",
    "clean": "npm run clean:app && npm run clean:dist",
    "installr:upload": "gulp installr:upload",
    "installr": "npm run installr:upload",
    "bower:install": "bower install",
    "bower:preen": "preen",
    "bower:move": "",
    "bower": "npm run bower:install && npm run bower:preen && npm run bower:move && npm run clean:bower"
  },
  "license": "Copyright Konstantin BÃ¼schel",
  "engines": {
    "titaniumsdk": ">=3.5.1",
    "node": ">=0.8"
  },
  "os": [
    "osx"
  ],
  "preferGlobal": false,
  "private": true,
  "devDependencies": {
    "bower": "*",
    "del": "^1.1.1",
    "gulp": "^3.8.8",
    "gulp-bump": "^0.3.1",
    "gulp-load-plugins": "*",
    "gulp-notify": "^2.2.0",
    "gulp-xml-editor": "^2.1.2",
    "iconfont": "*",
    "nconf": "^0.6.9",
    "plist": "*",
    "preen": "*",
    "ti-stealth": "*",
    "tiapp.xml": "*",
    "tich": "^1.0.4",
    "ticons": "*",
    "titanium": "*",
    "appcelerator": "*"
  },
  "dependencies": {
    "alloy-widget-nl.fokkezb.drawer": "^1.1.1"
  },
  "titanium": {
    "superagent": "ti-superagent"
  }
}
